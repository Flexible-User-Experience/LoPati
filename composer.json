{
    "name": "flux/lo-pati",
    "license": "proprietary",
    "description": "Lo Pati webapp project",
    "type": "project",
    "autoload": {
      "psr-4": {
        "": "src/"
      },
      "files": [
        "app/AppKernel.php"
      ]
    },
    "require": {
        "a2lix/translation-form-bundle": "1.*",
        "components/jquery": "^2.0",
        "cypresslab/gitelephant-bundle": "dev-develop",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "doctrine/migrations": "^1.0",
        "doctrine/orm": "^2.4.8",
        "egeloen/ckeditor-bundle": "^4.0",
        "egulias/email-validator": "^1.2",
        "friendsofsymfony/elastica-bundle": "3.0.*",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "friendsofsymfony/rest-bundle": "1.4.*",
        "genemu/form-bundle": "2.2.*",
        "hautelook/alice-bundle": "^1.0",
        "hipaway-travel/mandrill-bundle": "1.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "ircmaxell/password-compat": "~1.0",
        "jms/i18n-routing-bundle": "1.*",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "2.*",
        "liip/functional-test-bundle": "^1.0",
        "liip/imagine-bundle": "^1.0",
        "php": ">=5.3.9",
        "presta/sitemap-bundle": "^1.3",
        "raulfraile/ladybug-bundle": "1.*",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/admin-bundle": "2.4.x-dev",
        "sonata-project/datagrid-bundle": "2.2.x-dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.4.x-dev",
        "sonata-project/intl-bundle" : "2.2.*",
        "sonata-project/seo-bundle": "2.0.x-dev",
        "sonata-project/user-bundle": "2.3.x-dev",
        "stfalcon/tinymce-bundle": "0.3.*",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.7",
        "symfony/monolog-bundle": "~2.4",
        "symfony/symfony": "2.8.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "twbs/bootstrap": "^3.0",
        "twig/extensions": "^1.3",
        "vich/uploader-bundle": "^0.14.0"
    },
    "require-dev": {
      "sensio/generator-bundle": "~3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
      "symfony-app-dir": "app",
      "symfony-web-dir": "web",
      "symfony-assets-install": "relative",
      "incenteev-parameters": {
        "file": "app/config/parameters.yml"
      }
    }
}
